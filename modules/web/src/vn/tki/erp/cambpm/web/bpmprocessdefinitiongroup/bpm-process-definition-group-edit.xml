<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="vn.tki.erp.cambpm.web.bpmprocessdefinitiongroup.BpmProcessDefinitionGroupEdit"
        datasource="bpmProcessDefinitionGroupDs"
        focusComponent="fieldGroup"
        messagesPack="vn.tki.erp.cambpm.web.bpmprocessdefinitiongroup">
    <dsContext>
        <datasource id="bpmProcessDefinitionGroupDs"
                    class="vn.tki.erp.cambpm.entity.BpmProcessDefinitionGroup"
                    view="_local"/>
    </dsContext>
    <dialogMode forceDialog="true"
                height="AUTO"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="bpmProcessDefinitionGroupDs">
            <column width="250px">
                <field property="name"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
